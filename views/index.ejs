<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/style.css' />
    <script src="/bower_components/angular/angular.min.js"></script>
    <script src="/js/module.js"></script>
    <!-- 
    <script src="/directives/myTable.js"></script>
    <script src="/controllers/mainCtrl.js"></script>
     -->
  </head>
  <body ng-app="someApp">
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <!-- <my-table data='users'></my-table> -->
    <label for="">Description:</label>
    <input type="text" ng-model="todo.description">
    <button ng-click="addTodo(todo)">Add Todo</button>

    <table>
      <thead>
        <tr>
          <td ng-repeat='(header, value) in list[0] track by $index'
              ng-click='sort(header)'>{{header}}</td>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat='item in list | orderBy:sorttext track by $index'>
          <td ng-repeat='value in item '>{{value}}</td>
        </tr>
      </tbody>
    </table>
  </body>
</html>
